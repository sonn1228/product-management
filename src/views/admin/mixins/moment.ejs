<% function formatDateTime(time) { %>
  <p>
    <%= moment(time).format('DD/MM/YYYY, HH:mm:ss') %>
  </p>
  <% } %>

    <% function formatDate(time) { %>
      <p>
        <%= moment(time).format('DD/MM/YYYY') %>
      </p>
      <% } %>

        <% function formatTime(time) { %>
          <p>
            <%= moment(time).format('HH:mm:ss') %>
          </p>
          <% } %>